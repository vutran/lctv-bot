{
  "extends": "eslint:recommended",
  "parser": "babel-eslint",
  "rules": {
    "strict": [2, "global"],
    "no-trailing-spaces": 2,
    "indent": [2, 2, {"SwitchCase": 1}],
    "no-dupe-class-members": 2,
    "no-console": 0
  },
  "env": {
    "node": true,
    "es6": true,
    "jest": true,
    "jasmine": true
  }
}
